{
  "version": 2,
  "env": {
    "CLIENT_DOMAIN": "https://chat-socket-blue.vercel.app/"
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "PUT", "DELETE", "POST"]
    }
  ]
}
