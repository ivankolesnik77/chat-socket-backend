{
  "version": 2,
  "env": {
    "CLIENT_DOMAIN": "https://chat-socket-blue.vercel.app/"
  },
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js",
      "methods": ["GET", "PUT", "DELETE", "POST"]
    }
  ]
}
